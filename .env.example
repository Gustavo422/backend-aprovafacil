# Configurações do Servidor
PORT=5000
NODE_ENV=development

# Configurações do Banco de Dados
DATABASE_URL=postgresql://usuario:senha@localhost:5432/aprova_ja
DATABASE_POOL_MIN=2
DATABASE_POOL_MAX=10

# Configurações de Segurança
JWT_SECRET=seu_segredo_jwt_super_secreto
JWT_EXPIRES_IN=7d
COOKIE_SECRET=seu_segredo_para_cookies
CORS_ORIGIN=http://localhost:3000

# Configurações de Cache (opcional)
REDIS_URL=redis://localhost:6379
CACHE_TTL=3600

# Configurações de E-mail (opcional)
SMTP_HOST=smtp.exemplo.com
SMTP_PORT=587
SMTP_USER=usuario@exemplo.com
SMTP_PASS=sua_senha
SMTP_FROM=nao-responda@aprova-ja.com

# Configurações de Upload (opcional)
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=10485760 # 10MB

# Chaves de API (opcional)
GOOGLE_API_KEY=sua_chave_google
STRIPE_SECRET_KEY=sua_chave_stripe
